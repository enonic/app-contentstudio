{
  "name": "app-contentstudio",
  "version": "6.0.0",
  "description": "Enonic XP - Content Studio App",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/enonic/app-contentstudio"
  },
  "scripts": {
    "build": "pnpm --color /^build:.*$/",
    "build:worker": "webpack --config webpack.worker.config.js --color --stats=minimal",
    "build:client": "webpack --color --stats=minimal",
    "build:server": "tsc --project ./src/main/resources/lib/tsconfig.json",
    "check": "pnpm --color /^check:.*$/",
    "check:types": "pnpm --color /^check:types:.*$/",
    "check:types:client": "tsc --pretty --skipLibCheck --noEmit",
    "check:types:worker": "tsc --pretty --skipLibCheck --noEmit --project ./src/main/resources/assets/shared-socket/tsconfig.json",
    "check:lint": "eslint **/*.ts --quiet --cache"
  },
  "dependencies": {
    "@enonic/lib-admin-ui": "workspace:*",
    "chart.js": "^4.5.1",
    "enonic-admin-artifacts": "^2.4.0",
    "jquery": "^3.7.1",
    "jquery-simulate": "^1.0.2",
    "jquery-ui": "^1.14.1",
    "lib-contentstudio": "workspace:*",
    "nanostores": "^0.11.4",
    "q": "^1.5.1"
  },
  "devDependencies": {
    "@enonic-types/core": "7.15.3",
    "@enonic-types/global": "7.15.3",
    "@enonic-types/lib-admin": "7.15.3",
    "@enonic-types/lib-auth": "7.15.3",
    "@enonic-types/lib-common": "7.15.3",
    "@enonic-types/lib-context": "7.15.3",
    "@enonic-types/lib-event": "7.15.3",
    "@enonic-types/lib-grid": "7.15.3",
    "@enonic-types/lib-i18n": "7.15.3",
    "@enonic-types/lib-node": "7.15.3",
    "@enonic-types/lib-portal": "7.15.3",
    "@enonic-types/lib-value": "7.15.3",
    "@enonic-types/lib-websocket": "7.15.3",
    "@enonic/eslint-config": "^2.1.0",
    "@swc/core": "^1.15.2",
    "@types/ckeditor": "^4.9.10",
    "@types/jquery": "^3.5.33",
    "@types/jqueryui": "^1.12.24",
    "@types/q": "^1.5.8",
    "autoprefixer": "^10.4.22",
    "browserslist-config-enonic": "^1.0.8",
    "circular-dependency-plugin": "^5.2.2",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "cssnano": "^7.1.2",
    "eslint": "^9.39.1",
    "globals": "^15.15.0",
    "less-loader": "^12.3.0",
    "mini-css-extract-plugin": "^2.9.4",
    "postcss-loader": "^8.2.0",
    "postcss-normalize": "^13.0.1",
    "postcss-sort-media-queries": "^5.2.0",
    "swc-loader": "^0.2.6",
    "terser-webpack-plugin": "^5.3.14",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  },
  "browserslist": [
    "extends browserslist-config-enonic"
  ],
  "engines": {
    "node": ">= 22.19.0",
    "pnpm": ">= 10.16.0",
    "npm": ">= 10.9.3"
  }
}
